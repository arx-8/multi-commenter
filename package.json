    "yup": "0.27.0"
  },
  "devDependencies": {
    "@types/draft-js": "0.10.37",
    "@types/emoji-mart": "2.11.0",
    "@types/gapi": "0.0.39",
    "@types/gapi.auth2": "0.0.50",
    "@types/gapi.client.youtube": "3.0.1",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
